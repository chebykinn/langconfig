{
  "name": "@typeconf/config-emitter",
  "version": "0.1.0",
  "description": "Typeconf internal config emitter",
  "homepage": "https://typeconf.dev",
  "readme": "https://github.com/typeconf/typeconf/blob/main/README.md",
  "author": "Ivan Chebykin",
  "license": "Apache-2.0",
  "keywords": [
      "typeconf",
      "config-emitter"
  ],
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "dependencies": {
    "@typespec/compiler": "^0.59.1",
    "@typespec-tools/emitter-typescript": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "typescript": "^5.3.3",
    "prettier": "^3.0.3"
  },
  "scripts": {
    "build": "tsc && cp package-dist.json dist/package.json",
    "watch": "tsc --watch",
    "test": "node --test",
    "lint": "eslint src/ test/ --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "format": "prettier .  --write",
    "format:check": "prettier --check ."
  }
}
